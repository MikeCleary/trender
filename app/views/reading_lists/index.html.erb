<% @reading_lists.each do |reading_list| %>
  <%= render :partial => 'reading_lists/reading_list_brief', :locals => {:reading_list => reading_list} %>
<% end %>
<nav class="pages">
  <div id="page_number">
    <%= link_to "Previous", readers_path(:page_number => @page_number - 1) %>
    <div class="page_number">
      <%= @page_number %>
    </div>
    <%= link_to "Next", readers_path(:page_number => @page_number + 1) %>
  </div>
</nav>
